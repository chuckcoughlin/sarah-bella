<launch>
  <param name="robot/name" value="SarahBella" type="str"/>
  <param name="robot/type" value="turtlebot3" type="str"/>
  <param name="robot/application" value="turtlebot3_follower" type="str"/>
  <param name="robot/platform" value="RaspberryPi" type="str"/>
<node pkg="laser_filters" type="scan_to_scan_filter_chain" output="screen" name="laser_filter">
      <remap from="scan" to="/scan" />
      <rosparam command="load" file="$(find turtlebot3_follow_filter)/filter/turtlebot3_follow_filter.yaml" />
      <remap from="scan_filtered" to="/scan_filtered" />
</node>
</launch>
